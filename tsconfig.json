{
  // This file is not used in compilation. It is here just for a nice editor experience.
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "types": [
      "node",
      "@docusaurus/module-type-aliases",
      "@docusaurus/theme-classic"
    ]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "coverage/**",
    "**/lib/**/*",
    "website/**",
    "**/__mocks__/**/*",
    "**/__fixtures__/**/*",
    "examples/**",
    "packages/create-docusaurus/templates/**"
  ]
}